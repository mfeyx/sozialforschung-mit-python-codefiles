    "    d = {i:json.loads(s) for (i, s) in enumerate(list_obj, 1)}\n",
    "\n",
    "d"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open(\"export.json\", \"w\") as f:\n",
    "    for val in d.values():\n",
    "        f.write(json.dumps(val))"
   ]
  }
